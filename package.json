  "dependencies": {
    "gatsby": "^2.1.4",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "onchange": "^5.2.0",
    "prettier": "1.16.4"
  }
}
